{
  "name": "notational-markdown",
  "version": "0.0.1",
  "description": "Notational Velocity inspired Markdown",
  "main": "main.js",
  "browserify": {
    "transform": [ "browserify-shim" ]
  },
  "browserify-shim": {
    "./node_modules/simplemde/dist/simplemde.js": "SimpleMDE"
  },
  "scripts": {
    "start": "electron main.js",
    "watch": "watchify app/app.js -t babelify -o public/js/bundle.js --debug --verbose"
  },
  "author": "Will Simons",
  "license": "CC0-1.0",
  "dependencies": {
    "axios": "^0.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "classnames": "^2.2.3",
    "electron-prebuilt": "^0.37.8",
    "electron-reload": "^0.2.0",
    "jquery": "^2.2.3",
    "react": "^0.14.8",
    "react-autocomplete": "^1.0.0-rc2",
    "react-dom": "^0.14.7",
    "simplemde": "^1.10.1"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "browserify-shim": "^3.8.12"
  }
}
